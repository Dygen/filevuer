{
    "name": "dygen/filevuer",
    "description": "A remote FTP & S3 file manager using VueJS",
    "keywords": [
        "Laravel",
        "S3",
        "FTP"
    ],
    "homepage": "https://github.com/Dygen/filevuer",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Dygen",
        "email": "dygen@gmail.com"
    }],
    "require": {
        "php": ">=8.1",
        "aws/aws-sdk-php": "^3.64",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "maennchen/zipstream-php": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "jwhulette\\filevuer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jwhulette\\filevuer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jwhulette\\filevuer\\FileVuerServiceProvider"
            ]
        }
    }
}
